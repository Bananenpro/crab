func main() throws {
	println("hello, world!", toLower("HelLo, WORld!"));
	println("HELLO, WORLD!", toUpper("HelLo, WoRld!"));

	println(true, contains("Hello, World", "e"));
	println(true, contains("Hello, World", "ell"));

	println(false, contains("Hello, World", "y"));
	println(false, contains("Hello, World", "elly"));

	println(true, contains([1, 2, 3], 2));
	println(false, contains([1, 2, 3], 4));

	println(2, indexOf("Hello, World", "l"));

	println(-1, indexOf("Hello, World", "elly"));

	println(1, indexOf([1, 2, 3], 2));
	println(-1, indexOf([1, 2, 3], 4));

	println("Hello, World!", trim("        \t\t Hello, World!\t\t               "));
	println(5.5, trim(5.5));

	println("Henno, Wornd!", replace("Hello, World!", "l", "n"));
	println([1,1,1,1], replace([1,2,1,2], 2, 1));

	println(["a","b","c","d"], split("a,b,c,d", ","));
	println(["Hello, World!"], split("Hello, World!", "-"));
	println([[1,2],[3,4],[5,6]], split([1,2,-1,3,4,-1,5,6], -1));

	println("a-true-5", join(["a", true, 5], "-"));
}
